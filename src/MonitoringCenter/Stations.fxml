<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<StackPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MonitoringCenter.StationsController">
   <children>
          <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
              <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Monitoring Stations">
                      <VBox.margin>
                          <Insets bottom="15.0" top="15.0" />
                      </VBox.margin>
                  </Text>
            <TableView prefHeight="200.0" prefWidth="200.0" fx:id="msTable">
              <columns>
                  <TableColumn prefWidth="75.0" text="Name" fx:id="nameCol" />
                  <TableColumn prefWidth="75.0" text="Location" fx:id="locationCol"/>
              </columns>
            </TableView>
                  <HBox alignment="CENTER">
                      <VBox.margin>
                          <Insets top="10.0" />
                      </VBox.margin>
                      <children>
                          <Button fx:id="viewStationsBtn" mnemonicParsing="false" onAction="#enable" text="Enable">
                              <HBox.margin>
                                  <Insets left="5.0" right="5.0" />
                              </HBox.margin>
                          </Button>
                          <Button fx:id="getReadingBtn2" layoutX="71.0" layoutY="10.0" mnemonicParsing="false" onAction="#disable" text="Disable" />
                      </children>
                  </HBox>
              </children>
          </VBox>
   </children>
</StackPane>
